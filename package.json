{
  "name": "@o-rango/update-notify",
  "version": "0.1.1",
  "description": "Check if an package needs to be update ",
  "author": "romulo cintra",
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/o-rango/update-notify.git"
  },
  "keywords": [
    "npm",
    "update-notify",
    "npm-check",
    "npm-info",
    "update-dependencies"
  ],
  "bugs": {
    "url": "https://github.com/o-rango/update-notify/issues"
  },
  "homepage": "https://github.com/o-rango/update-notify#readme",
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "lint": "prettier  --single-quote --write ./**/*.js",
    "release": "standard-version",
    "postpublish": "git push origin master --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "ci-info": "^2.0.0",
    "configstore": "^5.0.0",
    "execa": "4.0.0",
    "import-lazy": "^4.0.0",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.2",
    "prettier": "1.19.1",
    "standard-version": "7.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(|js)$",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 75,
        "statements": -10
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!src/**/*.stories.js",
      "src/**/*.{js,jsx}",
      "!src/**/index.{js,jsx}"
    ]
  }
}
